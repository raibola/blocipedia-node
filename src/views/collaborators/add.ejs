<% if(currentUser.id == wiki.userId || currentUser.isAdmin()) { %>
<form action="/wikis/<%= wiki.id %>/collaborators/add" method="post">
  <div class="form-group">
    <label for="username">Add Collaborator</label>
    <input type="text" class="form-control" name="username" aria-describedby="titleHelp" placeholder="Enter username">
  </div>
  <button type="submit" class="btn btn-primary">Add</button>
</form> 
<% } %>
